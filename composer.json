        "ehough/contemplate": "~1.0.0",
        "ehough/curly":       "~1.0.0",
        "ehough/epilog":      "~1.5",
        "ehough/finder":      "~2.0",
        "ehough/iconic":      "~2.3",
        "ehough/pulsar":      "~2.3",
        "ehough/shortstop":   "~2.0",
        "ehough/stash":       "~0.10",
        "ehough/tickertape":  "~2.3"
