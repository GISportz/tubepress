            "tubepress_youtube_"   : "src/core/youtube/classes",

            "tubepress_api_"       : "src/platform/classes",
            "tubepress_impl_"      : "src/platform/classes",

            "tubepress_test_boot_"             : "tests/platform/fixtures/scripts/boot/add-ons/bootmocks/classes",
            "tubepress_test_core_ioc_"         : "tests/core/ioc/classes",
            "tubepress_test_core_options_"     : "tests/core/options/classes",
            "tubepress_test_core_options_ui_"  : "tests/core/options-ui/classes",
            "tubepress_test_core_translation_" : "tests/core/translation/classes",
            "tubepress_test_"                  : "tests/phpunit/classes",

            "tubepress_api_const_template_"      : "src/core/deprecated/old-classes",
