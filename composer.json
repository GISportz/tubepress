            "tubepress_jwplayer_"  : "src/main/add-ons/jwplayer/classes",
            "tubepress_vimeo_"     : "src/main/add-ons/vimeo/classes",
            "tubepress_wordpress_" : "src/main/add-ons/wordpress/classes",
            "tubepress_youtube_"   : "src/main/add-ons/youtube/classes",

            "tubepress_api_"       : "src/main/platform/classes",
            "tubepress_impl_"      : "src/main/platform/classes",

            "tubepress_test_boot_"             : "src/test/platform/fixtures/scripts/boot/add-ons/bootmocks/classes",
            "tubepress_test_core_ioc_"         : "src/test/add-ons/ioc/classes",
            "tubepress_test_core_options_"     : "src/test/add-ons/options/classes",
            "tubepress_test_core_options_ui_"  : "src/test/add-ons/options-ui/classes",
