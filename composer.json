{
    "name": "ehough/tubepress",

    "description": "TubePress is an open source PHP library that displays gorgeous YouTube and Vimeo videos and galleries in your blog. It's available as a WordPress plugin or as a stand-alone PHP library. Please visit http://tubepress.org for more information.",

    "homepage": "https://github.com/ehough/tubepress",

    "license": "MPL-2.0",

    "authors": [
        {
            "name": "TubePress LLC",
            "email": "eric@tubepress.org",
            "homepage": "http://tubepress.org"
        }
    ],

    "require": {

        "ehough/chaingang": "*",
        "ehough/contemplate": "*",
        "ehough/curly": "*",
        "ehough/epilog": "*",
        "ehough/fimble": "*",
        "ehough/iconic": "*",
        "ehough/jameson": "*",
        "ehough/pulsar": "*",
        "ehough/shortstop": "*",
        "ehough/stash": "*",
        "ehough/tickertape": "*"
    },

    "require-dev": {
        
        "mockery/mockery": ">=0.7.2"
    },

    "autoload": {

        "psr-0": {

            "tubepress": "src/main/php/classes",
            "org_tubepress" : "src/main/php/deprecated_code/classes"
        }
    }
}
