{
    "name" : "tubepress/youtube",
    "version": "1.0.0",
    "title" : "YouTube",
    "authors" : [
        {
            "name" : "TubePress LLC",
            "url" : "http://tubepress.com"
        }
    ],
    "licenses": [
        {
            "type" : "MPL-2.0",
            "url" : "http://www.mozilla.org/MPL/2.0/"
        }
    ],
    "description": "Allows TubePress work with YouTube",
    "container-builder": {
        "extensions" : [
            "tubepress_youtube2_ioc_YouTubeExtension"
        ]
