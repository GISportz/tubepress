{
    "name" : "tubepress/core-theme",
    "version": "1.0.0",
    "title" : "TubePress Theme Capability",
    "author" : {
        "name" : "TubePress LLC",
        "url" : "http://tubepress.com"
    },
    "licenses": [
        {
            "type" : "MPL-2.0",
            "url" : "http://www.mozilla.org/MPL/2.0/"
        }
    ],
    "description": "TubePress core theme functionality.",
    "inversion-of-control": {
        "container-extensions" : [
            "tubepress_app_theme_ioc_ThemeExtension"
        ],
        "compiler-passes" : {
            "tubepress_app_theme_ioc_compiler_ThemesPrimerPass" : 60000
        }
    },
    "autoload":{
        "classmap" : {
            "tubepress_app_theme_api_Constants"                              : "classes/tubepress/app/theme/api/Constants.php",
            "tubepress_app_theme_api_ThemeInterface"                         : "classes/tubepress/app/theme/api/ThemeInterface.php",
            "tubepress_app_theme_api_ThemeLibraryInterface"                  : "classes/tubepress/app/theme/api/ThemeLibraryInterface.php",
            "tubepress_app_theme_impl_options_ui_FieldProvider"              : "classes/tubepress/app/theme/impl/options/ui/FieldProvider.php",
            "tubepress_app_theme_impl_listeners_options_AcceptableValues"    : "classes/tubepress/app/theme/impl/listeners/options/AcceptableValues.php",
            "tubepress_app_theme_impl_listeners_options_LegacyThemeListener" : "classes/tubepress/app/theme/impl/listeners/options/LegacyThemeListener.php",
            "tubepress_app_theme_impl_ThemeBase"                             : "classes/tubepress/app/theme/impl/ThemeBase.php",
            "tubepress_app_theme_impl_ThemeLibrary"                          : "classes/tubepress/app/theme/impl/ThemeLibrary.php",
            "tubepress_app_theme_impl_ThemeRegistry"                         : "classes/tubepress/app/theme/impl/ThemeRegistry.php",
            "tubepress_app_theme_ioc_ThemeExtension"                         : "classes/tubepress/app/theme/ioc/ThemeExtension.php",
            "tubepress_app_theme_ioc_compiler_ThemesPrimerPass"              : "classes/tubepress/app/theme/ioc/compiler/ThemesPrimerPass.php"
        }
    }
}