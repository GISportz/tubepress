{
    "name" : "tubepress/core-options",
    "version": "1.0.0",
    "title" : "TubePress Core Options Functionality",
    "author" : {
        "name" : "TubePress LLC",
        "url" : "http://tubepress.com"
    },
    "licenses": [
        {
            "type" : "MPL-2.0",
            "url" : "http://www.mozilla.org/MPL/2.0/"
        }
    ],
    "description": "TubePress core options functionality.",
    "inversion-of-control": {
        "container-extensions" : [
            "tubepress_app_options_ioc_OptionsExtension"
        ],
        "compiler-passes" : {

            "tubepress_app_options_ioc_compiler_EasyAcceptableValuesPass" : 90000,
            "tubepress_app_options_ioc_compiler_EasyReferencesPass"       : 90000,
            "tubepress_app_options_ioc_compiler_EasyValidatorsPass"       : 90000,
            "tubepress_app_options_ioc_compiler_EasyTrimmersPass"         : 90000,
            "tubepress_app_options_ioc_compiler_ReferenceCreatorPass"     : 80000
        }
    },
    "autoload":{
        "classmap" : {

            "tubepress_app_options_api_AcceptableValuesInterface"   : "classes/tubepress/app/options/api/AcceptableValuesInterface.php",
            "tubepress_app_options_api_Constants"                   : "classes/tubepress/app/options/api/Constants.php",
            "tubepress_app_options_api_ContextInterface"            : "classes/tubepress/app/options/api/ContextInterface.php",
            "tubepress_app_options_api_PersistenceBackendInterface" : "classes/tubepress/app/options/api/PersistenceBackendInterface.php",
            "tubepress_app_options_api_PersistenceInterface"        : "classes/tubepress/app/options/api/PersistenceInterface.php",
            "tubepress_app_options_api_ReferenceInterface"          : "classes/tubepress/app/options/api/ReferenceInterface.php",

            "tubepress_app_options_impl_AcceptableValues"                  : "classes/tubepress/app/options/impl/AcceptableValues.php",
            "tubepress_app_options_impl_easy_EasyAcceptableValuesListener" : "classes/tubepress/app/options/impl/easy/EasyAcceptableValuesListener.php",
            "tubepress_app_options_impl_easy_EasyReference"                : "classes/tubepress/app/options/impl/easy/EasyReference.php",
            "tubepress_app_options_impl_easy_EasyValidator"                : "classes/tubepress/app/options/impl/easy/EasyValidator.php",
            "tubepress_app_options_impl_easy_EasyTrimmer"                  : "classes/tubepress/app/options/impl/easy/EasyTrimmer.php",
            "tubepress_app_options_impl_internal_AbstractOptionReader"     : "classes/tubepress/app/options/impl/internal/AbstractOptionReader.php",
            "tubepress_app_options_impl_listeners_BasicOptionValidity"     : "classes/tubepress/app/options/impl/listeners/BasicOptionValidity.php",
            "tubepress_app_options_impl_listeners_Logger"                  : "classes/tubepress/app/options/impl/listeners/Logger.php",
            "tubepress_app_options_impl_listeners_StringMagic"             : "classes/tubepress/app/options/impl/listeners/StringMagic.php",
            "tubepress_app_options_impl_Reference"                         : "classes/tubepress/app/options/impl/Reference.php",
            "tubepress_app_options_impl_Context"                           : "classes/tubepress/app/options/impl/Context.php",
            "tubepress_app_options_impl_Persistence"                       : "classes/tubepress/app/options/impl/Persistence.php",

            "tubepress_app_options_ioc_OptionsExtension"                  : "classes/tubepress/app/options/ioc/OptionsExtension.php",
            "tubepress_app_options_ioc_compiler_ReferenceCreatorPass"     : "classes/tubepress/app/options/ioc/compiler/ReferenceCreatorPass.php",
            "tubepress_app_options_ioc_compiler_EasyAcceptableValuesPass" : "classes/tubepress/app/options/ioc/compiler/EasyAcceptableValuesPass.php",
            "tubepress_app_options_ioc_compiler_EasyReferencesPass"       : "classes/tubepress/app/options/ioc/compiler/EasyReferencesPass.php",
            "tubepress_app_options_ioc_compiler_EasyTrimmersPass"         : "classes/tubepress/app/options/ioc/compiler/EasyTrimmersPass.php",
            "tubepress_app_options_ioc_compiler_EasyValidatorsPass"       : "classes/tubepress/app/options/ioc/compiler/EasyValidatorsPass.php",
            "tubepress_app_options_ioc_compiler_AbstractEasyPass"         : "classes/tubepress/app/options/ioc/compiler/AbstractEasyPass.php"
        }
    }
}