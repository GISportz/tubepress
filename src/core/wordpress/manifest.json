{
    "name" : "tubepress/wordpress",
    "version": "1.0.0",
    "title" : "WordPress",
    "author" : {
        "name" : "TubePress LLC",
        "url" : "http://tubepress.com"
    },
    "licenses": [
        {
            "type" : "MPL-2.0",
            "url" : "http://www.mozilla.org/MPL/2.0/"
        }
    ],
    "description": "Allows TubePress to integrate with WordPress",
    "inversion-of-control": {
        "compiler-passes" : {
            "tubepress_wordpress_ioc_compiler_EnvironmentCompilerPass" : 70000
        },
        "container-extensions" : [
            "tubepress_wordpress_ioc_WordPressExtension"
        ]
    },
    "autoload": {
        "classmap" : {
            "tubepress_wordpress_api_Constants"                             : "classes/tubepress/wordpress/api/Constants.php",
            "tubepress_wordpress_impl_Callback"                             : "classes/tubepress/wordpress/impl/Callback.php",
            "tubepress_wordpress_impl_listeners_html_WpHtmlListener"        : "classes/tubepress/wordpress/impl/listeners/html/WpHtmlListener.php",
            "tubepress_wordpress_impl_listeners_wp_PublicActionsAndFilters" : "classes/tubepress/wordpress/impl/listeners/wp/PublicActionsAndFilters.php",
            "tubepress_wordpress_impl_listeners_wp_AdminActionsAndFilters"  : "classes/tubepress/wordpress/impl/listeners/wp/AdminActionsAndFilters.php",
            "tubepress_wordpress_impl_listeners_template_options_OptionsUiTemplateListener" : "classes/tubepress/wordpress/impl/listeners/template/options/OptionsUiTemplateListener.php",
            "tubepress_wordpress_impl_message_WordPressMessageService"    : "classes/tubepress/wordpress/impl/message/WordPressMessageService.php",
            "tubepress_wordpress_impl_options_PersistenceBackend"         : "classes/tubepress/wordpress/impl/options/PersistenceBackend.php",
            "tubepress_wordpress_impl_options_ui_WpFieldProvider"         : "classes/tubepress/wordpress/impl/options/ui/WpFieldProvider.php",
            "tubepress_wordpress_impl_options_ui_fields_WpNonceField"     : "classes/tubepress/wordpress/impl/options/ui/fields/WpNonceField.php",
            "tubepress_wordpress_impl_wp_ActivationHook"                  : "classes/tubepress/wordpress/impl/wp/ActivationHook.php",
            "tubepress_wordpress_impl_wp_OptionsPage"                     : "classes/tubepress/wordpress/impl/wp/OptionsPage.php",
            "tubepress_wordpress_impl_wp_Widget"                          : "classes/tubepress/wordpress/impl/wp/Widget.php",
            "tubepress_wordpress_impl_wp_WpFunctions"                     : "classes/tubepress/wordpress/impl/wp/WpFunctions.php",
            "tubepress_wordpress_ioc_WordPressExtension"                  : "classes/tubepress/wordpress/ioc/WordPressExtension.php",
            "tubepress_wordpress_ioc_compiler_EnvironmentCompilerPass"    : "classes/tubepress/wordpress/ioc/compiler/EnvironmentCompilerPass.php"
        }
    }
}