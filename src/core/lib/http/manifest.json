{
    "name" : "tubepress/core-base-http",
    "version": "1.0.0",
    "title" : "TubePress HTTP Implementation",
    "author" : {
        "name" : "TubePress LLC",
        "url" : "http://tubepress.com"
    },
    "licenses": [
        {
            "type" : "MPL-2.0",
            "url" : "http://www.mozilla.org/MPL/2.0/"
        }
    ],
    "description": "TubePress core HTTP functionality.",
    "inversion-of-control": {
        "container-extensions" : [
            "tubepress_lib_http_ioc_HttpExtension"
        ]
    },
    "autoload":{
        "classmap" : {
            "tubepress_lib_http_api_Constants"                   : "classes/tubepress/lib/http/api/Constants.php",
            "tubepress_lib_http_api_HttpClientInterface"         : "classes/tubepress/lib/http/api/HttpClientInterface.php",
            "tubepress_lib_http_api_ResponseCodeInterface"       : "classes/tubepress/lib/http/api/ResponseCodeInterface.php",
            "tubepress_lib_http_api_exception_RequestException"  : "classes/tubepress/lib/http/api/exception/RequestException.php",
            "tubepress_lib_http_api_message_MessageInterface"    : "classes/tubepress/lib/http/api/message/MessageInterface.php",
            "tubepress_lib_http_api_message_RequestInterface"    : "classes/tubepress/lib/http/api/message/RequestInterface.php",
            "tubepress_lib_http_api_message_ResponseInterface"   : "classes/tubepress/lib/http/api/message/ResponseInterface.php",
            "tubepress_lib_http_api_oauth_v1_ClientInterface"    : "classes/tubepress/lib/http/api/oauth/v1/ClientInterface.php",
            "tubepress_lib_http_api_oauth_v1_Credentials"        : "classes/tubepress/lib/http/api/oauth/v1/Credentials.php",
            "tubepress_lib_http_impl_AbstractHttpClient"         : "classes/tubepress/lib/http/impl/AbstractHttpClient.php",
            "tubepress_lib_http_impl_oauth_v1_Client"            : "classes/tubepress/lib/http/impl/oauth/v1/Client.php",
            "tubepress_lib_http_impl_options_ui_FieldProvider"   : "classes/tubepress/lib/http/impl/options/ui/FieldProvider.php",
            "tubepress_lib_http_impl_puzzle_AbstractMessage"     : "classes/tubepress/lib/http/impl/puzzle/AbstractMessage.php",
            "tubepress_lib_http_impl_puzzle_PuzzleBasedRequest"  : "classes/tubepress/lib/http/impl/puzzle/PuzzleBasedRequest.php",
            "tubepress_lib_http_impl_puzzle_PuzzleBasedResponse" : "classes/tubepress/lib/http/impl/puzzle/PuzzleBasedResponse.php",
            "tubepress_lib_http_impl_puzzle_PuzzleHttpClient"    : "classes/tubepress/lib/http/impl/puzzle/PuzzleHttpClient.php",
            "tubepress_lib_http_impl_puzzle_RequestException"    : "classes/tubepress/lib/http/impl/puzzle/RequestException.php",
            "tubepress_lib_http_impl_ResponseCode"               : "classes/tubepress/lib/http/impl/ResponseCode.php",
            "tubepress_lib_http_ioc_HttpExtension"               : "classes/tubepress/lib/http/ioc/HttpExtension.php"
        }
    }
}