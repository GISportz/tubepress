            "tubepress_lib_template_impl_contemplate_TemplateFactory" : "classes/tubepress/lib/template/impl/contemplate/TemplateFactory.php",
            "tubepress_lib_template_ioc_TemplateExtension"            : "classes/tubepress/lib/template/ioc/TemplateExtension.php"
        }
    }
