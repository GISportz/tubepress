{
    "name" : "tubepress/core-ioc",
    "version": "1.0.0",
    "title" : "TubePress Core Inversion of Control Functionality",
    "author" : {
        "name" : "TubePress LLC",
        "url" : "http://tubepress.com"
    },
    "licenses": [
        {
            "type" : "MPL-2.0",
            "url" : "http://www.mozilla.org/MPL/2.0/"
        }
    ],
    "description": "TubePress core Inversion of Control functionality.",
    "inversion-of-control": {
        "container-extensions" : [
            "tubepress_core_ioc_VendorsExtension"
        ],
        "compiler-passes" : {

            "tubepress_core_ioc_compiler_TaggedServicesConsumerPass" : 70000,
            "tubepress_core_ioc_compiler_RegisterListenersPass"      : 70000
        }
    },
    "autoload":{
        "classmap" : {
            "tubepress_core_ioc_api_Constants" : "classes/tubepress/core/ioc/api/Constants.php",
            "tubepress_core_ioc_VendorsExtension" : "classes/tubepress/core/ioc/VendorsExtension.php",
            "tubepress_core_ioc_compiler_RegisterListenersPass" : "classes/tubepress/core/ioc/compiler/RegisterListenersPass.php",
            "tubepress_core_ioc_compiler_TaggedServicesConsumerPass" : "classes/tubepress/core/ioc/compiler/TaggedServicesConsumerPass.php"
        }
    }
}