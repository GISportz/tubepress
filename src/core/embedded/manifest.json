{
    "name" : "tubepress/core-embedded",
    "version": "1.0.0",
    "title" : "TubePress Embedded Player Functionality",
    "author" : {
        "name" : "TubePress LLC",
        "url" : "http://tubepress.com"
    },
    "licenses": [
        {
            "type" : "MPL-2.0",
            "url" : "http://www.mozilla.org/MPL/2.0/"
        }
    ],
    "description": "TubePress core embedded player functionality.",
    "inversion-of-control": {
        "container-extensions" : [
            "tubepress_core_embedded_ioc_EmbeddedExtension"
        ]
    },
    "autoload":{
