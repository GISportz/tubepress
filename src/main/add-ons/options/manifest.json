{
    "name" : "tubepress/core-options",
    "version": "1.0.0",
    "title" : "TubePress Core Options Functionality",
    "author" : {
        "name" : "TubePress LLC",
        "url" : "http://tubepress.com"
    },
    "licenses": [
        {
            "type" : "MPL-2.0",
            "url" : "http://www.mozilla.org/MPL/2.0/"
        }
    ],
    "description": "TubePress core options functionality.",
    "inversion-of-control": {
        "container-extensions" : [
            "tubepress_core_options_ioc_OptionsExtension"
        ],
        "compiler-passes" : {

            "tubepress_core_options_ioc_compiler_EasyAcceptableValuesPass" : 90000,
            "tubepress_core_options_ioc_compiler_EasyReferencesPass"       : 90000,
            "tubepress_core_options_ioc_compiler_EasyValidatorsPass"       : 90000,
            "tubepress_core_options_ioc_compiler_EasyTrimmersPass"         : 90000,
            "tubepress_core_options_ioc_compiler_ReferenceCreatorPass"     : 80000
        }
    },
    "autoload":{
        "psr-0" : {
            "tubepress_core_options_" : "classes"
        },
        "classmap" : {

            "tubepress_core_options_api_AcceptableValuesInterface"   : "classes/tubepress/core/options/api/AcceptableValuesInterface.php",
            "tubepress_core_options_api_Constants"                   : "classes/tubepress/core/options/api/Constants.php",
            "tubepress_core_options_api_ContextInterface"            : "classes/tubepress/core/options/api/ContextInterface.php",
            "tubepress_core_options_api_PersistenceBackendInterface" : "classes/tubepress/core/options/api/PersistenceBackendInterface.php",
            "tubepress_core_options_api_PersistenceInterface"        : "classes/tubepress/core/options/api/PersistenceInterface.php",
            "tubepress_core_options_api_ReferenceInterface"          : "classes/tubepress/core/options/api/ReferenceInterface.php",

            "tubepress_core_options_impl_AcceptableValues" : "classes/tubepress/core/options/impl/AcceptableValues.php",
            "tubepress_core_options_impl_easy_EasyAcceptableValuesListener" : "classes/tubepress/core/options/impl/easy/EasyAcceptableValuesListener.php",
            "tubepress_core_options_impl_easy_EasyReference" : "classes/tubepress/core/options/impl/easy/EasyReference.php",
            "tubepress_core_options_impl_easy_EasyValidator" : "classes/tubepress/core/options/impl/easy/EasyValidator.php",
            "tubepress_core_options_impl_easy_EasyTrimmer" : "classes/tubepress/core/options/impl/easy/EasyTrimmer.php",
            "tubepress_core_options_impl_internal_AbstractOptionReader" : "classes/tubepress/core/options/impl/internal/AbstractOptionReader.php",
            "tubepress_core_options_impl_listeners_BasicOptionValidity" : "classes/tubepress/core/options/impl/listeners/BasicOptionValidity.php",
            "tubepress_core_options_impl_listeners_Logger" : "classes/tubepress/core/options/impl/listeners/Logger.php",
            "tubepress_core_options_impl_listeners_StringMagic" : "classes/tubepress/core/options/impl/listeners/StringMagic.php",

            "tubepress_core_options_impl_Reference" : "classes/tubepress/core/options/impl/Reference.php",

            "tubepress_core_options_ioc_compiler_ReferenceCreatorPass" : "classes/tubepress/core/options/ioc/compiler/ReferenceCreatorPass.php",
            "tubepress_core_options_impl_Context" : "classes/tubepress/core/options/impl/Context.php",
            "tubepress_core_options_impl_Persistence" : "classes/tubepress/core/options/impl/Persistence.php"
        }
    }
}